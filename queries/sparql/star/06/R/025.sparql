PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
SELECT ?x ?y1 ?y2
WHERE {
  ?x
    p:P681 [ ps:P681 wd:Q21750259 ] ;
    p:P680 [ ps:P680 wd:Q14864149 ] ;
    p:P361 [ ps:P361 ?y1 ] ;
    p:P279 [ ps:P279 ?y2 ] ;
    p:P703 [ ps:P703 ?y3 ] ;
    p:P702 [ ps:P702 ?y4 ]
}
