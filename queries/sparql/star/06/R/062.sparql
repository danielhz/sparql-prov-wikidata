PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
SELECT ?x ?y1 ?y2
WHERE {
  ?x
    p:P682 [ ps:P682 wd:Q847556 ] ;
    p:P527 [ ps:P527 wd:Q24774001 ] ;
    p:P703 [ ps:P703 ?y1 ] ;
    p:P279 [ ps:P279 ?y2 ] ;
    p:P680 [ ps:P680 ?y3 ] ;
    p:P361 [ ps:P361 ?y4 ]
}
