PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
SELECT ?x ?y1 ?y2
WHERE {
  ?x
    p:P684 [ ps:P684 wd:Q62210774 ] ;
    p:P1057 [ ps:P1057 wd:Q61923798 ] ;
    p:P688 [ ps:P688 ?y1 ] ;
    p:P2548 [ ps:P2548 ?y2 ] ;
    p:P703 [ ps:P703 ?y3 ] ;
    p:P279 [ ps:P279 ?y4 ]
}
