PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
SELECT ?x ?y1 ?y2
WHERE {
  ?x
    p:P681 [ ps:P681 wd:Q14878256 ] ;
    p:P527 [ ps:P527 wd:Q24771528 ] ;
    p:P702 [ ps:P702 ?y1 ] ;
    p:P361 [ ps:P361 ?y2 ] ;
    p:P682 [ ps:P682 ?y3 ] ;
    p:P680 [ ps:P680 ?y4 ]
}
