PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
SELECT ?x ?y1 ?y2
WHERE {
  ?x
    p:P702 [ ps:P702 wd:Q28983486 ] ;
    p:P527 [ ps:P527 wd:Q24772108 ] ;
    p:P680 [ ps:P680 ?y1 ] ;
    p:P682 [ ps:P682 ?y2 ] ;
    p:P361 [ ps:P361 ?y3 ] ;
    p:P279 [ ps:P279 ?y4 ]
}
