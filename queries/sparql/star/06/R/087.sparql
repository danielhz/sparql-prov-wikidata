PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
SELECT ?x ?y1 ?y2
WHERE {
  ?x
    p:P170 [ ps:P170 wd:Q1983389 ] ;
    p:P276 [ ps:P276 wd:Q3695427 ] ;
    p:P186 [ ps:P186 ?y1 ] ;
    p:P195 [ ps:P195 ?y2 ] ;
    p:P6216 [ ps:P6216 ?y3 ] ;
    p:P1071 [ ps:P1071 ?y4 ]
}
