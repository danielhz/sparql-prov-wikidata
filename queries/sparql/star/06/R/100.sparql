PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
SELECT ?x ?y1 ?y2
WHERE {
  ?x
    p:P197 [ ps:P197 wd:Q72594679 ] ;
    p:P137 [ ps:P137 wd:Q879270 ] ;
    p:P81 [ ps:P81 ?y1 ] ;
    p:P17 [ ps:P17 ?y2 ] ;
    p:P127 [ ps:P127 ?y3 ] ;
    p:P131 [ ps:P131 ?y4 ]
}
