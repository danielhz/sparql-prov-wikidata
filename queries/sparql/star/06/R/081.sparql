PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
SELECT ?x ?y1 ?y2
WHERE {
  ?x
    p:P156 [ ps:P156 wd:Q19984922 ] ;
    p:P361 [ ps:P361 wd:Q1138524 ] ;
    p:P155 [ ps:P155 ?y1 ] ;
    p:P1433 [ ps:P1433 ?y2 ] ;
    p:P50 [ ps:P50 ?y3 ] ;
    p:P6216 [ ps:P6216 ?y4 ]
}
