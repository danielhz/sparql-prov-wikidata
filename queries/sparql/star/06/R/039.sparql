PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
SELECT ?x ?y1 ?y2
WHERE {
  ?x
    p:P106 [ ps:P106 wd:Q4009406 ] ;
    p:P1344 [ ps:P1344 wd:Q495366 ] ;
    p:P21 [ ps:P21 ?y1 ] ;
    p:P27 [ ps:P27 ?y2 ] ;
    p:P1532 [ ps:P1532 ?y3 ] ;
    p:P641 [ ps:P641 ?y4 ]
}
