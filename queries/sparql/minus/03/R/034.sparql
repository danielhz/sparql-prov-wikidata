PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
SELECT ?x ?y1
WHERE {
  {
    ?x
      p:P527 [ ps:P527 wd:Q24784524 ] ;
      p:P361 [ ps:P361 ?y1 ] ;
      p:P682 [ ps:P682 ?y2a ]
  }
  MINUS
  {
    ?x
      p:P527 [ ps:P527 wd:Q24784524 ] ;
      p:P361 [ ps:P361 ?y1 ] ;
      p:P682 [ ps:P682 ?y2b ]
  }
}
