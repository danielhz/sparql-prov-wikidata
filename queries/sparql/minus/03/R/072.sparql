PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
SELECT ?x ?y1
WHERE {
  {
    ?x
      p:P21 [ ps:P21 wd:Q6581072 ] ;
      p:P106 [ ps:P106 ?y1 ] ;
      p:P1412 [ ps:P1412 ?y2a ]
  }
  MINUS
  {
    ?x
      p:P21 [ ps:P21 wd:Q6581072 ] ;
      p:P106 [ ps:P106 ?y1 ] ;
      p:P1412 [ ps:P1412 ?y2b ]
  }
}
