  {
    ?x
      wdt:P641 wd:Q12100 ;
      wdt:P21 wd:Q6581072 ;
      wdt:P27 ?y1 ;
      wdt:P735 ?y2 ;
      wdt:P19 ?y3b ;
      wdt:P106 ?y4b
  }
}
