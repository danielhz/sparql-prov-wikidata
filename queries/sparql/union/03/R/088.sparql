PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
SELECT *
WHERE {
  {
    SELECT ?x ?y1
    WHERE {
      ?x
        p:P2860 [ ps:P2860 wd:Q51802514 ] ;
        p:P1433 [ ps:P1433 ?y1 ] ;
        p:P921 [ ps:P921 ?y2a ]
    }
  }
  UNION
  {
    SELECT ?x ?y1
    WHERE {
      ?x
        p:P2860 [ ps:P2860 wd:Q51802514 ] ;
        p:P1433 [ ps:P1433 ?y1 ] ;
        p:P921 [ ps:P921 ?y2b ]
    }
  }
}
