PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
SELECT *
WHERE {
  {
    SELECT ?x ?y1
    WHERE {
      ?x
        p:P131 [ ps:P131 wd:Q1520038 ] ;
        p:P1435 [ ps:P1435 ?y1 ] ;
        p:P17 [ ps:P17 ?y2a ]
    }
  }
  UNION
  {
    SELECT ?x ?y1
    WHERE {
      ?x
        p:P131 [ ps:P131 wd:Q1520038 ] ;
        p:P1435 [ ps:P1435 ?y1 ] ;
        p:P17 [ ps:P17 ?y2b ]
    }
  }
}
