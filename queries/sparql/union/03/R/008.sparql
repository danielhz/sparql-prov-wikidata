PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
SELECT *
WHERE {
  {
    SELECT ?x ?y1
    WHERE {
      ?x
        p:P703 [ ps:P703 wd:Q21383317 ] ;
        p:P279 [ ps:P279 ?y1 ] ;
        p:P688 [ ps:P688 ?y2a ]
    }
  }
  UNION
  {
    SELECT ?x ?y1
    WHERE {
      ?x
        p:P703 [ ps:P703 wd:Q21383317 ] ;
        p:P279 [ ps:P279 ?y1 ] ;
        p:P688 [ ps:P688 ?y2b ]
    }
  }
}
