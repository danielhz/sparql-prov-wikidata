PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
SELECT *
WHERE {
  {
    SELECT ?x ?y1
    WHERE {
      ?x
        wdt:P641 wd:Q2736 ;
        wdt:P3450 ?y1 ;
        wdt:P2348 ?y2a
    }
  }
  UNION
  {
    SELECT ?x ?y1
    WHERE {
      ?x
        wdt:P641 wd:Q2736 ;
        wdt:P3450 ?y1 ;
        wdt:P2348 ?y2b
    }
  }
}
