PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
SELECT *
WHERE {
  {
    SELECT ?x ?y1
    WHERE {
      ?x
        wdt:P6886 wd:Q143 ;
        wdt:P21 ?y1 ;
        wdt:P27 ?y2a
    }
  }
  UNION
  {
    SELECT ?x ?y1
    WHERE {
      ?x
        wdt:P6886 wd:Q143 ;
        wdt:P21 ?y1 ;
        wdt:P27 ?y2b
    }
  }
}
