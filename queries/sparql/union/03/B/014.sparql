PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
SELECT *
WHERE {
  {
    SELECT ?x ?y1
    WHERE {
      ?x
        wdt:P361 wd:Q6727012 ;
        wdt:P27 ?y1 ;
        wdt:P551 ?y2a
    }
  }
  UNION
  {
    SELECT ?x ?y1
    WHERE {
      ?x
        wdt:P361 wd:Q6727012 ;
        wdt:P27 ?y1 ;
        wdt:P551 ?y2b
    }
  }
}
