PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
SELECT *
WHERE {
  {
    SELECT ?x ?y1
    WHERE {
      ?x
        wdt:P22 wd:Q75755458 ;
        wdt:P21 ?y1 ;
        wdt:P40 ?y2a
    }
  }
  UNION
  {
    SELECT ?x ?y1
    WHERE {
      ?x
        wdt:P22 wd:Q75755458 ;
        wdt:P21 ?y1 ;
        wdt:P40 ?y2b
    }
  }
}
