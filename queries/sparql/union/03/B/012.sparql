PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
SELECT *
WHERE {
  {
    SELECT ?x ?y1
    WHERE {
      ?x
        wdt:P1001 wd:Q52870 ;
        wdt:P17 ?y1 ;
        wdt:P279 ?y2a
    }
  }
  UNION
  {
    SELECT ?x ?y1
    WHERE {
      ?x
        wdt:P1001 wd:Q52870 ;
        wdt:P17 ?y1 ;
        wdt:P279 ?y2b
    }
  }
}
