PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
SELECT *
WHERE {
  {
    SELECT ?x ?y1
    WHERE {
      ?x
        p:P735 [ ps:P735 wd:Q339346 ] ;
        p:P19 [ ps:P19 wd:Q49220 ] ;
        p:P734 [ ps:P734 ?y1 ] ;
        p:P1532 [ ps:P1532 ?y2 ] ;
        p:P106 [ ps:P106 ?y3a ] ;
        p:P641 [ ps:P641 ?y4a ]
    }
  }
  UNION
  {
    SELECT ?x ?y1
    WHERE {
      ?x
        p:P735 [ ps:P735 wd:Q339346 ] ;
        p:P19 [ ps:P19 wd:Q49220 ] ;
        p:P734 [ ps:P734 ?y1 ] ;
        p:P1532 [ ps:P1532 ?y2 ] ;
        p:P106 [ ps:P106 ?y3b ] ;
        p:P641 [ ps:P641 ?y4b ]
    }
  }
}
