PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
SELECT *
WHERE {
  {
    SELECT ?x ?y1
    WHERE {
      ?x
        p:P703 [ ps:P703 wd:Q21102993 ] ;
        p:P279 [ ps:P279 wd:Q8054 ] ;
        p:P527 [ ps:P527 ?y1 ] ;
        p:P680 [ ps:P680 ?y2 ] ;
        p:P702 [ ps:P702 ?y3a ] ;
        p:P361 [ ps:P361 ?y4a ]
    }
  }
  UNION
  {
    SELECT ?x ?y1
    WHERE {
      ?x
        p:P703 [ ps:P703 wd:Q21102993 ] ;
        p:P279 [ ps:P279 wd:Q8054 ] ;
        p:P527 [ ps:P527 ?y1 ] ;
        p:P680 [ ps:P680 ?y2 ] ;
        p:P702 [ ps:P702 ?y3b ] ;
        p:P361 [ ps:P361 ?y4b ]
    }
  }
}
